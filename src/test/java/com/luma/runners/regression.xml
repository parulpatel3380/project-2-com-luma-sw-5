<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="regression" verbose="1" >
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>


    <listeners>
        <listener class-name="com.luma.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="regression test" >
        <classes>
            <class name="com.luma.testsuite.GearTest"/>
            <class name="com.luma.testsuite.MenTest"/>
            <class name="com.luma.testsuite.Women_Jackets_Test"/>
        </classes>
    </test>
</suite>